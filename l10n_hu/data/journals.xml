<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Journal sequence types -->
        <record model="ir.sequence.type" id="szallito_naplo_type">
            <field name="name">Szállító Napló ssz</field>
            <field name="code">szallito</field>
        </record>
        <record model="ir.sequence.type" id="zaro_naplo_type">
            <field name="name">Záró Napló ssz</field>
            <field name="code">zar</field>
        </record>
        <record model="ir.sequence.type" id="vevo_kulso_naplo_type">
            <field name="name">Vevő Napló-Külső ssz</field>
            <field name="code">vevo-kulso</field>
        </record>
        <record model="ir.sequence.type" id="bankkartyas_szallito_naplo_type">
            <field name="name">Bankkártyás Szállító Napló</field>
            <field name="code">bankkartyas-szallito</field>
        </record>        

        <!-- Journal sequences -->
        <record model="ir.sequence" id="szallito">
            <field name="name">Készpénzes Szállító</field>
            <field name="code">szallito</field>
            <field name="prefix">KPSZ/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="active">True</field>
        </record>
        <record model="ir.sequence" id="zaro_naplo">
            <field name="name">Záró Napló</field>
            <field name="code">zar</field>
            <field name="prefix">ZAR/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="active">True</field>
        </record>
        <record model="ir.sequence" id="vevo-kulso">
            <field name="name">Vevő sorszám-Külső</field>
            <field name="code">vevo-kulso</field>
            <field name="prefix">VEKU/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="active">True</field>
        </record>


        <!-- Journals -->
        <record model="account.journal" id="kpsz">
            <field name="name">Készpénzes Szállító Napló</field>
            <field name="code">KPSZ</field>
            <field name="type">purchase</field>          
            <field name="sequence_id" ref="szallito"/>
            <field name="centralisation" eval="False"/>
            <field name="entry_posted" eval="False"/>
            <field name="update_posted" eval="True"/>
        </record>
        <record model="account.journal" id="zar">
            <field name="name">Záró Napló</field>
            <field name="code">ZAR</field>
            <field name="type">situation</field>        
            <field name="sequence_id" ref="zaro_naplo"/>
            <field name="centralisation" eval="True"/>
            <field name="entry_posted" eval="False"/>
            <field name="update_posted" eval="True"/>
            <field eval="1" name="allow_date"/>
        </record>
        <record model="account.journal" id="ve-kulso">
            <field name="name">Vevő Napló-Külső</field>
            <field name="code">VEKU</field>
            <field name="type">sale</field>          
            <field name="sequence_id" ref="vevo-kulso"/>
            <field name="centralisation" eval="False"/>
            <field name="entry_posted" eval="False"/>
            <field name="update_posted" eval="True"/>
            <field name="manual_external" eval="True"/>            
        </record>
    </data>
</openerp>